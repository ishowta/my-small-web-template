{
  "name": "my-small-web-template",
  "version": "0.0.0",
  "scripts": {
    "dev": "run-p dev:* watch",
    "dev:vite": "vite",
    "build": "tsc && vite build --outDir public",
    "serve": "vite preview",
    "deploy": "yarn build && firebase deploy --only hosting,firestore",
    "lint": "yarn eslint --cache --ext ts,tsx --fix src",
    "format": "yarn prettier --write --loglevel warn .",
    "type-check": "tsc",
    "check": "run-p lint format type-check",
    "gen:theme-typings": "chakra-cli tokens src/theme.ts",
    "postinstall": "yarn run gen:theme-typings",
    "prepare": "husky install",
    "watch": "run-p watch:*",
    "watch:lint": "watch \"yarn lint\" src",
    "watch:type-check": "yarn type-check --watch --preserveWatchOutput"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.6.7",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@types/react-router-dom": "^5.1.9",
    "firebase": "^9.0.2",
    "firebase-tools": "^9.18.0",
    "focus-visible": "^5.2.0",
    "framer-motion": "^4",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-firebase-hooks": "^4.0.1",
    "react-router-dom": "^5.3.0",
    "watch": "^1.0.2"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^1.5.0",
    "@types/node": "^16.9.2",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "2.3.2",
    "typescript": "^4.4.2",
    "vite": "^2.5.2"
  }
}
